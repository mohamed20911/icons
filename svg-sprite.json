{
  "mode": {
    "symbol": {
      "dest": ".",
      "sprite": "bootstrap-icons.svg"
    }
  },
  "shape": {
    "transform": [
      {
        "svgo": {
          "multipass": true,
          "plugins": [
            {
              "name": "preset-default",
              "params": {
                "overrides": {
                  "removeAttrs": {
                    "attrs": [
                      "data-name",
                      "fill",
                      "clip-rule"
                    ]
                  },
                  "removeUnknownsAndDefaults": {
                    "keepRoleAttr": true
                  },
                  "removeViewBox": false
                }
              }
            },
            "cleanupListOfValues",
            "removeXMLNS",
            "sortAttrs"
          ]
        }
      }
    ]
  },
  "svg": {
    "namespaceClassnames": false,
    "xmlDeclaration": false
  }
}
